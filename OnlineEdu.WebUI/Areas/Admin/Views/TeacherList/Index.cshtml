@model List<AppUser>

@{
    ViewData["Title"] = "Eğitmenler";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@{
	int count = 0;
}

<table class="table table-hover">
	<thead>
		<tr>
			<th>#</th>
			<th>Ad Soyad</th>
			<th>Kullanıcı Adı </th>
			<th>Email</th>
			<th></th>

		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			count++;
			<tr>
				<td>@count</td>
				<td>@item.FirstName @item.LastName</td>
				<td>@item.UserName</td>
				<td>@item.Email</td>

			</tr>
		}
	</tbody>
</table>
